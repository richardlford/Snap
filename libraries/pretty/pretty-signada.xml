  <block-definition s="&#x1F916; connect to %'ip'" type="command" category="other">
    <header/>
    <code/>
    <translations>ca:&#x1F916; connecta't a _&#xD;</translations>
    <inputs>
      <input type="%s">192.168.4.1</input>
    </inputs>
    <script>
      <block s="doApplyExtension">
        <l>src_load(url)</l>
        <list>
          <l>libraries/signada.js</l>
        </list>
      </block>
      <custom-block s="&#x1F916; disconnect"/>
      <block s="doApplyExtension">
        <l>sgd_connect(ip)</l>
        <list>
          <block var="ip"/>
        </list>
      </block>
    </script>
  </block-definition>
  <block-definition s="&#x1F916; disconnect" type="command" category="other">
